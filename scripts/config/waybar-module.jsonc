// Wizado Waybar Module Configuration
// Add this module to your waybar config's "modules-right" array
// and include the module definition in your config file.
//
// Icon: nf-fa-hat_wizard ()
// Example: "modules-right": ["...", "custom/wizado", "..."]

{
  "custom/wizado": {
    "format": "{}",
    "return-type": "json",
    "exec": "wizado status",
    "on-click": "wizado-menu-float",
    "on-click-right": "wizado-menu-float",
    "interval": 60,
    "tooltip": true
  }
}
